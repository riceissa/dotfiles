browser iceweasel
unbind-key R
bind-key ^R reload-all

unbind-key J
unbind-key K

unbind-key l
bind-key l open
unbind-key h
bind-key h quit

unbind-key j
bind-key j down
unbind-key k
bind-key k up
#bind-key j next
#bind-key k prev

bind-key J next
bind-key K prev

bind-key ; mark-feed-read

unbind-key i
bind-key i mark-all-feeds-read
